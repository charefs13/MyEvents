model Entreprise {
  id            Int             @id @default(autoincrement())
  raisonSociale String
  siret         String
  type          String
  adresse       String
  ville         String
  cp            Int
  utilisateur   Utilisateur      @relation(fields: [utilisateurId], references: [id])
  utilisateurId Int @unique
//   prestations   Prestation[]
//   commentaires  CommentaireAvis[]
}
 